<a href="" class="btn btn-primary btn-sm m-2"
  ><i class="fas fa-plus-circle fa-sm"></i> Add Contact</a
>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email Address</th>
      <th scope="col">Contact No</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <%for (let count = 0; count< contactList.length;count++){%>
    <tr>
      <th scope="row"><%= count+1 %></th>
      <td><%= contactList[count].contactName %></td>
      <td><%= contactList[count].emailAddress %></td>
      <td><%= contactList[count].phone %></td>
      <td class="text-center">
        <a href="" class="btn btn-primary btn-sm"
          ><i class="fas fa-edit fa-sm"></i> Edit</a
        >

        <a href="" class="btn btn-danger btn-sm"
          ><i class="fas fa-eraser fa-sm"></i> Delete</a
        >
      </td>
    </tr>
    <%}%>
  </tbody>
</table>
